# NODE环境
NODE_ENV = 'development'

# 变量必须以 VITE_APP_ 为前缀才能暴露给外部读取
# 环境标识
VITE_APP_NODE_ENV = 'dev'

# 网站标题
VITE_APP_TITLE = 'Qs-Admin后台管理系统模板'

# 项目基路径
VITE_APP_BASE_URL = '/'

# 接口基路径
VITE_APP_BASE_API = '/api'

# 是否开启开发环境Mock
VITE_APP_ENABLE_DEV_MOCK = true

# 是否开启生产环境Mock
VITE_APP_ENABLE_PROD_MOCK = false

# 打包目录名
VITE_APP_OUTDIR = 'dist_dev'